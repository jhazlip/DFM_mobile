          },
          "category": {
            "#cdata-section": "Cops and Courts"
          },
          "guid": {
            "@isPermaLink": "true",
            "#text": "http://www.denverpost.com/news/ci_23005002"
          },
          "pubDate": "Thu, 11 Apr 2013 13:31:44 -0600",
          "lastUpdated": "Thu, 11 Apr 2013 14:56:25 -0600",
          "meta": [
            {
              "@id": "articleId",
              "#cdata-section": "23005002"
