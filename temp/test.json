          },
          "author": {
            "#cdata-section": "By Kieran Nicholson, The Denver Post"
          },
          "category": {
            "#cdata-section": "Local News"
          },
          "guid": {
            "@isPermaLink": "true",
            "#text": "http://www.denverpost.com/news/ci_23004024"
          },
          "pubDate": "Thu, 11 Apr 2013 11:32:09 -0600",
          "lastUpdated": "Thu, 11 Apr 2013 12:36:26 -0600",
          "meta": [
            {
